<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <key id="weight_key" for="edge" attr.name="weight" attr.type="int">
        <default>1</default>
    </key>

  <graph id="graph" edgedefault="undirected">
    <node id="A"/>
    <node id="B"/>
    <node id="C"/>
    <node id="D"/>
    <node id="E"/>

    <edge id="e1" source="A" target="B">
        <data key="weight_key">7</data>
    </edge>
    <edge id="e2" source="A" target="C">
        <data key="weight_key">2</data>
    </edge>
    <edge id="e3" source="B" target="D">
        <data key="weight_key">3</data>
    </edge>
    <edge id="e4" source="B" target="C">
        <data key="weight_key">1</data>
    </edge>
    <edge id="e5" source="C" target="D">
        <data key="weight_key">5</data>
    </edge>
    <edge id="e6" source="C" target="E">
        <data key="weight_key">10</data>
    </edge>
    <edge id="e7" source="D" target="E">
        <data key="weight_key">4</data>
    </edge>
  </graph>
</graphml>